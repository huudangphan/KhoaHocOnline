}

<section class="page-header services-header" data-parallax="scroll" data-image-src="~/Assets/images/header/services-folding-img.jpg">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1 class="text-center"> Các Khóa học <br> tại F</h1>
            </div>
        </div>
    </div>
</section>

<section class="services">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
